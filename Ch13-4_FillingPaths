<!DOCTYPE HTML>
<head>
<title>canvas Fill Paths</title>
</head>
<body>
<script src="https://code.jquery.com/jquery-2.1.0.js"></script> 
	<canvas id="canvas" width="400" height="400"></canvas>
<script>
var canvas = document.getElementById("canvas"); 
var ctx=canvas.getContext("2d");

ctx.fillStyle="skyBlue";//backgroundColor;
ctx.beginPath(); //starting drawing
	ctx.moveTo(100,100);
	ctx.lineTo(100,60);
	ctx.lineTo(130,30);
	ctx.lineTo(160,60);
	ctx.lineTo(160,100);
	ctx.lineTo(100,100);
ctx.fill() //closing the neginPath() method;
</script>
</body>
</html>
