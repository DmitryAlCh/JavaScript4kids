<script>
var names=["Adaam", "Volodja", "Ken"];
$("#main-heading").text("My Friends");// when Identifier use "#"
for (var i=0;i<names.length;i++){
	console.log(i);
	$("body").append("<p>"+names[i]+"</p>"); //when DOM element use "body" directly
	$("p").hide(3000).fadeIn(3000);
	}
	$("p").append(" smells");
	

</script>
